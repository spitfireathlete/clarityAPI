<h1>Listing users</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
	  <th>Email</th>
	  <th>First Name</th>
	  <th>Last Name</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
		<td><%= user.email %></td>
		<td><%= user.first_name %></td>
		<td><%= user.last_name %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>